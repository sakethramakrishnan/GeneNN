{
    "architectures": [
        "GPTNeoXForCausalLM"
    ],
    "hidden_size": 8196,
    "num_attention_heads": 64,
    "num_hidden_layers": 64,
    "intermediate_size": 8000,
    "attention_probs_dropout_prob": 0.1,
    "bos_token_id": 0,
    "eos_token_id": 0,
    "hidden_act": "gelu_fast",
    "hidden_dropout_prob": 0.1,
    "initializer_range": 0.02,
    "layer_norm_eps": 1e-05,
    "max_position_embeddings": 2048,
    "model_type": "gpt_neox",
    "rotary_emb_base": 10000,
    "rotary_pct": 0.25,
    "tie_word_embeddings": false,
    "torch_dtype": "float16",
    "transformers_version": "4.21.1",
    "use_cache": true,
    "vocab_size": 69, 
    "use_deepspeed_checkpointing": true, 
    "_deepspeed_checkpoint_comment": "Note that deepspeed checkpointing is only supported with maxzvyagin's transformers fork, optimizations are underway to remove this dependency"
